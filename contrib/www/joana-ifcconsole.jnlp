<?xml version="1.0" encoding="UTF-8"?>
<jnlp spec="1.0+" codebase="https://pp.ipd.kit.edu/~joana/webstart/" href="joana-ifcconsole.jnlp">
	<information>
		<title>Joana IFC Console</title>
		<description>Simple interactive IFC analysis tool</description>
		<vendor>KIT, Programming Paradigms Group - IPD Snelting</vendor>
		<homepage href="https://pp.ipd.kit.edu/" />
		<icon href="icons/joana-ifcconsole.png" />
		<shortcut online="false">
			<desktop />
			<menu submenu="Joana IFC Tools" />
		</shortcut>
		<offline-allowed />
	</information>
	<resources>
		<!-- Application Resources -->
		<java version="1.8+" java-vm-args="-Xmx1024m" />
		<jar href="webstart-jars/joana.ui.ifc.wala.console.jar" main="true" />
		<property name="debug" value="true" />
		<property name="declassEnabled" value="false" />
	</resources>
	<application-desc
		name="Joana IFC Console">
	</application-desc>
	<security>
		<all-permissions />
	</security>
	<update check="always" />
</jnlp>
